<odoo>
    <record id="cron_studio_automation" model="ir.cron">
        <field name="name">Execute Studio Automations</field>
        <field name="model_id" ref="model_studio_automation"/>
        <field name="state">code</field>
        <field name="code">
            model.execute_automations()
        </field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="active">True</field>
    </record>
</odoo>
