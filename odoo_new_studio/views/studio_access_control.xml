<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_odoo_new_studio_access_control_tree" model="ir.ui.view">
        <field name="name">odoo.new.studio.access.control.tree</field>
        <field name="model">odoo.new.studio.access.control</field>
        <field name="arch" type="xml">
            <tree string="Access Controls">
                <field name="name"/>
                <field name="model"/>
                <field name="group_id"/>
            </tree>
        </field>
    </record>

    <record id="view_odoo_new_studio_access_control_form" model="ir.ui.view">
        <field name="name">odoo.new.studio.access.control.form</field>
        <field name="model">odoo.new.studio.access.control</field>
        <field name="arch" type="xml">
            <form string="Access Control">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="model"/>
                        <field name="group_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
