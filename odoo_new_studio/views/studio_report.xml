<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="studio_report_tree_view" model="ir.ui.view">
        <field name="name">studio.report.tree</field>
        <field name="model">studio.report</field>
        <field name="arch" type="xml">
            <tree string="Reports">
                <field name="name"/>
                <field name="model_id"/>
            </tree>
        </field>
    </record>

    <record id="studio_report_form_view" model="ir.ui.view">
        <field name="name">studio.report.form</field>
        <field name="model">studio.report</field>
        <field name="arch" type="xml">
            <form string="Report">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="model_id"/>
                        <!--field name="report_template"/-->
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
