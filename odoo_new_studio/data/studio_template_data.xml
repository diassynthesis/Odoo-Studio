<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="template_report_invoice" model="studio.template">
        <field name="name">Invoice Report Template</field>
        <field name="template_type">report</field>
        <field name="content">This is a default invoice report template.</field>
    </record>

    <record id="template_email_welcome" model="studio.template">
        <field name="name">Welcome Email Template</field>
        <field name="template_type">email</field>
        <field name="content">Welcome to our service! Here is your welcome email.</field>
    </record>

    <record id="template_other_example" model="studio.template">
        <field name="name">Custom Example Template</field>
        <field name="template_type">other</field>
        <field name="content">This is a sample template for custom use.</field>
    </record>
</odoo>
